<h1>Sign Up to get started!</h1>

<%= form_for User.new do |f| %>   

<%= f.label :First_Name %>   

<%= f.text_field :fname %>   

<br>

<%= f.label :Last_Name %>   

<%= f.text_field :lname %>  

<br>

<%= f.label :email %>   

<%= f.text_field :email %> 

<br> 

<%= f.label :password %>   

<%= f.text_field :password %>  

<br>

<%= f.submit %> 
<% end %>


